<%- include('../partials/header') %>

<h1>Skills as of <%= date %></h1>
<ul>
  <% skills.forEach(function(s) { %>
    <li>
      <a href="/skills/<%=  s.id %>"><%= s.skill %></a>
      -
      <%= s.achieved ? 'Achieved' : 'Not Achieved ..Yet' %> 
    </li>
    <% }); %> 
  </ul>
  <a href="/skills/new">Add a Skill</a>
  
  <%- include('../partials/footer') %>
